{% extends 'baseadmin.html.twig'  %}


{% block title %}}Hello CtrWalletController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <div style="display: flex;align-items: center;justify-content: center">
        <table border="1">
            <thead>
            <th>id_wallet</th>
            <th>solde</th>
            <th>card_number</th>
            <th>csc</th>
            <th>expire</th>
            </thead>
            <body>
            {% for wal in b %}
                <tr>
                    <td>{{wal.idWallet}}</td>
                    <td>{{wal.solde}}</td>
                    <td>{{wal.cardNumber}}</td>
                    <td>{{wal.csc}}</td>
                    <td>{{wal.expire}}</td>
                </tr>
            {% endfor %}
            </body>
        </table>
    </div>
</div>
{% endblock %}
